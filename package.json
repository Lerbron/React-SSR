{
  "name": "react-ssr",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p webpack-develop-client webpack-develop-server server-develop",
    "webpack-develop-client": "webpack --watch --progress --colors --config webpack.dev.config.js",
    "webpack-develop-server": "webpack --watch --progress --colors --config webpack.node.config.js",
    "server-develop": "nodemon dist/server.bundle.js",
    "build": "NODE_ENV='production' npm-run-all -p webpack-production-client webpack-production-server",
    "webpack-production-client": "webpack --config webpack.dev.config.js",
    "webpack-production-server": "webpack --config webpack.dev.config.js"
    
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "ejs": "^2.5.1",
    "es6-promise": "^4.1.1",
    "express": "^4.14.0",
    "history": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^2.7.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "webpack": "3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.24.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.22.0",
    "ignore-loader": "^0.1.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "postcss-loader": "^2.1.5",
    "redux-logger": "^2.6.1",
    "style-loader": "^0.13.1",
    "webpack-dev-server": "^1.15.0"
  }
}
